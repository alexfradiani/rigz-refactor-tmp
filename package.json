{
  "name": "rigz-refactoring-candidate",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "yarn prebuild && tsc",
    "serve:func": "yarn build && firebase emulators:start --only functions",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "seeds": "node ./dist/src/database/seeds/cli.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "lint": "eslint --ext .js,.ts .",
    "test": "ENV=test jest",
    "test:ci": "ENV=ci jest --verbose --coverage --runInBand",
    "migration:ci": "ENV=ci yarn typeorm migration:run"
  },
  "engines": {
    "node": "14"
  },
  "main": "dist/src/index.js",
  "dependencies": {
    "@google-cloud/firestore": "4.15.1",
    "@types/faker": "^5.5.8",
    "cors": "2.8.5",
    "csv-writer": "1.6.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-rate-limit": "5.3.0",
    "faker": "^5.5.3",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "helmet": "4.6.0",
    "mysql2": "2.3.2",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.38",
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "27.0.2",
    "@types/supertest": "2.0.11",
    "@types/yargs": "17.0.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "jest": "27.3.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-node": "10.3.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
